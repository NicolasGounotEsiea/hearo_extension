{
  "manifest_version": 3,
  "version": "0.1",
  "name": "Hearo",
  "description": "Podcast extension",
  "action": {
    "default_popup": "./popup.html",
    "default_icon": {
      "16": "./src/icons/icon.png",
      "32": "./src/icons/icon.png",
      "48": "./src/icons/icon.png",
      "128": "./src/icons/icon.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "identity"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "./src/icons/icon.png",
    "32": "./src/icons/icon.png",
    "48": "./src/icons/icon.png",
    "128": "./src/icons/icon.png"
  },
  "options_page": "options.html",
  "oauth2": {
    "client_id": "280710785691-sl9an0ageb1o22o8januotn5nrrgrabg.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": "-----BEGIN PUBLIC KEY-----\\n-----END PUBLIC KEY-----"  
}